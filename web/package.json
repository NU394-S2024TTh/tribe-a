{
	"name": "vite-reactts-eslint-prettier",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"build-deploy": "npm run build && npm run deploy",
		"build-dev": "npm run build && npm run dev",
		"deploy": "firebase deploy --only hosting",
		"init-db": "firebase init database",
		"serve": "vite preview",
		"test": "vitest --ui",
		"lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
		"lint:format": "prettier  --log-level warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
		"lint": "npm run lint:format && npm run lint:fix ",
		"type-check": "tsc"
	},
	"dependencies": {
		"@mui/icons-material": "^5.15.16",
		"@mui/material": "^5.15.16",
		"@mui/styled-engine": "^5.15.14",
		"@mui/utils": "^5.15.14",
		"@radix-ui/colors": "^3.0.0",
		"@radix-ui/themes": "^3.0.3",
		"@tailwindcss/forms": "^0.5.7",
		"autoprefixer": "^10.4.19",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.1",
		"depcheck": "^1.4.7",
		"firebase": "^10.11.1",
		"firebase-cli": "^1.2.0",
		"firebase-tools": "^13.8.0",
		"firestore": "^1.1.6",
		"prettier-plugin-tailwindcss": "^0.5.14",
		"react": "^18.3.1",
		"react-dom": "^18.2.0",
		"recharts": "^2.12.6",
		"tailwindcss-radix": "^3.0.3"
	},
	"devDependencies": {
		"@testing-library/react": "^14.2.2",
		"@types/react": "^18.2.73",
		"@types/react-dom": "^18.2.23",
		"@typescript-eslint/eslint-plugin": "^7.5.0",
		"@typescript-eslint/parser": "^7.5.0",
		"@vitejs/plugin-react": "^4.2.1",
		"@vitest/coverage-v8": "^1.4.0",
		"@vitest/ui": "^1.4.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react": "^7.34.1",
		"eslint-plugin-simple-import-sort": "^12.0.0",
		"jsdom": "^24.0.0",
		"postcss": "^8.4.38",
		"pre-commit": "^1.2.2",
		"prettier": "^3.2.5",
		"tailwindcss": "^3.4.3",
		"typescript": "^5.4.3",
		"vite": "^5.2.7",
		"vitest": "^1.4.0"
	},
	"pre-commit": "lint",
	"license": "MIT"
}
